{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "defaultProject": "app",
  "newProjectRoot": "projects",
  "projects": {
    "app": {
      "root": "",
      "sourceRoot": "src",
      "projectType": "application",
      "prefix": "app",
      "schematics": {},
      "architect": {
        "build": {
          "builder": "@angular-builders/custom-webpack:browser",
          "options": {
            "customWebpackConfig": {
              "path": "./scripts/webpack.config.js"
            },
            "outputPath": "www",
            "index": "src/index.html",
            "main": "src/main.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.app.custom.json",
            "styles": [
              {
                "input": "src/theme/variables.scss"
              },
              {
                "input": "src/global.scss"
              }
            ],
            "scripts": []
          },
          "configurations": {
            "dev": {},
            "prod": {
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "namedChunks": false,
              "aot": true,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                }
              ]
            },
            "en": {
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets/common",
                  "output": "assets/common",
                  "ignore": [
                    "**/*.schema.json"
                  ]
                },
                {
                  "glob": "**/*",
                  "input": "src/assets/en",
                  "output": "assets/en",
                  "ignore": [
                    "**/*.schema.json"
                  ]
                }
              ]
            },
            "it": {
              "assets": [
                {
                  "glob": "**/*",
                  "input": "src/assets/common",
                  "output": "assets/common",
                  "ignore": [
                    "**/*.schema.json"
                  ]
                },
                {
                  "glob": "**/*",
                  "input": "src/assets/it",
                  "output": "assets/it",
                  "ignore": [
                    "**/*.schema.json"
                  ]
                }
              ]
            },
            "ci": {
              "progress": false
            }
          }
        },
        "serve": {
          "builder": "@angular-builders/custom-webpack:dev-server",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "app:build:dev"
            },
            "prod": {
              "browserTarget": "app:build:prod"
            },
            "en": {
              "browserTarget": "app:build:en"
            },
            "it": {
              "browserTarget": "app:build:it"
            },
            "ci": {
              "progress": false
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "app:build"
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "src/test.ts",
            "polyfills": "src/polyfills.ts",
            "tsConfig": "tsconfig.spec.json",
            "karmaConfig": "karma.conf.js",
            "styles": [],
            "scripts": [],
            "assets": [
              {
                "glob": "favicon.ico",
                "input": "src/",
                "output": "/"
              },
              {
                "glob": "**/*",
                "input": "src/assets",
                "output": "/assets"
              }
            ]
          },
          "configurations": {
            "ci": {
              "progress": false,
              "watch": false
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "tsconfig.app.custom.json",
              "tsconfig.spec.json",
              "e2e/tsconfig.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        },
        "e2e": {
          "builder": "@angular-devkit/build-angular:protractor",
          "options": {
            "protractorConfig": "e2e/protractor.conf.js",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "dev": {
              "devServerTarget": "app:serve:dev"
            },
            "prod": {
              "devServerTarget": "app:serve:prod"
            },
            "en": {
              "devServerTarget": "app:serve:en"
            },
            "it": {
              "devServerTarget": "app:serve:it"
            },
            "ci": {
              "devServerTarget": "app:serve:ci"
            }
          }
        },
        "ionic-cordova-build": {
          "builder": "@ionic/angular-toolkit:cordova-build",
          "options": {
            "browserTarget": "app:build"
          },
          "configurations": {
            "dev": {
              "browserTarget": "app:build:dev"
            },
            "prod": {
              "browserTarget": "app:build:prod"
            },
            "en": {
              "browserTarget": "app:build:en"
            },
            "it": {
              "browserTarget": "app:build:it"
            }
          }
        },
        "ionic-cordova-serve": {
          "builder": "@ionic/angular-toolkit:cordova-serve",
          "options": {
            "cordovaBuildTarget": "app:ionic-cordova-build",
            "devServerTarget": "app:serve"
          },
          "configurations": {
            "dev": {
              "cordovaBuildTarget": "app:ionic-cordova-build:dev",
              "devServerTarget": "app:serve:dev"
            },
            "prod": {
              "cordovaBuildTarget": "app:ionic-cordova-build:prod",
              "devServerTarget": "app:serve:prod"
            },
            "en": {
              "cordovaBuildTarget": "app:ionic-cordova-build:en",
              "devServerTarget": "app:serve:en"
            },
            "it": {
              "cordovaBuildTarget": "app:ionic-cordova-build:it",
              "devServerTarget": "app:serve:it"
            }
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@ionic/angular-toolkit"
  },
  "schematics": {
    "@ionic/angular-toolkit:component": {
      "styleext": "scss"
    },
    "@ionic/angular-toolkit:page": {
      "styleext": "scss"
    }
  }
}