            "outFiles": [
                "${workspaceRoot}/BE/build/**/*.js"
            ]
        },
        {
            "type": "node",
            "request": "launch",
            "name": "Launch PSE-IE",
            "program": "${workspaceRoot}/IE/src/main.ts",
            "preLaunchTask": "build-tsc-ie",
            "console": "internalConsole",
            "stopOnEntry": false,
            "outFiles": [
                "${workspaceRoot}/IE/build/**/*.js"
            ]
        }
    ]
}