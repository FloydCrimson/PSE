{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "name": "Launch PSE-BE",
            "program": "${workspaceRoot}/BE/src/main.ts",
            "preLaunchTask": "build-tsc-be",
            "console": "internalConsole",
